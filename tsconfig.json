{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": true,
        "removeComments": true,
        "strict": true,
        "moduleResolution": "node",
        "baseUrl": "./",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist"
    },
    "exclude": [
        "node_modules",
        "dist"
    ],
    "include": [
        "src/**/*.ts"
    ],
}